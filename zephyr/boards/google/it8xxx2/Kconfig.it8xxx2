	select SOC_IT81302BX if BOARD_IT8XXX2_IT81302BX
	select SOC_IT81302CX if BOARD_IT8XXX2_IT81302CX
	select SOC_IT82002AW if BOARD_IT8XXX2_IT82002AW
	select SOC_IT82202AX if BOARD_IT8XXX2_IT82202AX
	select SOC_IT82202AX if BOARD_IT8XXX2_IT82202AX_512
	select SOC_IT82302AX if BOARD_IT8XXX2_IT82302AX
	select SOC_IT82202BW if BOARD_IT8XXX2_IT82202BW
