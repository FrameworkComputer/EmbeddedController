&uart2 {
	status = "okay";
	current-speed = <460800>;
	clock-frequency = <1843200>;

	one_wire_uart: one-wire-uart {
		compatible = "cros-ec,one-wire-uart";
	};
};

&ite_uart2_wrapper {
	status = "okay";
	pinctrl-0 = <&uart2_rx_gph5_default
		     &uart2_tx_gph6_default>;
	pinctrl-names = "default";
};
