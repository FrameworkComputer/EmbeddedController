/ {
	named-adc-channels {
		compatible = "named-adc-channels";

		/* Delete nodes defined on `mec172x.dtsi` to avoid conflict. */
		/delete-node/ ddr_soc;
		/delete-node/ ambient;
		/delete-node/ charger;
		/delete-node/ wwan;

		skin-therm-in1 {
			enum-name = "ADC_TEMP_SENSOR_4";
			io-channels = <&adc0 7>;
		};
		ddr-therm-in {
			enum-name = "ADC_TEMP_SENSOR_1_DDR_SOC";
			io-channels = <&adc0 6>;
		};
		vr-therm-in {
			enum-name = "ADC_TEMP_SENSOR_PP3300_REGULATOR";
			io-channels = <&adc0 5>;
		};
		ambient-therm-in0 {
			enum-name = "ADC_TEMP_SENSOR_2_AMBIENT";
			io-channels = <&adc0 4>;
		};
		et-therm-in3 {
			enum-name = "ADC_TEMP_SENSOR_3";
			io-channels = <&adc0 3>;
		};
		et-therm-in2 {
			enum-name = "ADC_TEMP_SENSOR_2";
			io-channels = <&adc0 2>;
		};
		et-therm-in1 {
			enum-name = "ADC_TEMP_SENSOR_1";
			io-channels = <&adc0 1>;
		};
	};
};

&adc0 {
	status = "okay";
	pinctrl-0 = <&adc01_gpio201
		     &adc02_gpio202
		     &adc03_gpio203
		     &adc04_gpio204
		     &adc05_gpio205
		     &adc06_gpio206
		     &adc07_gpio207>;
	pinctrl-names = "default";
};
