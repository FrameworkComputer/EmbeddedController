/* charger */
&CHARGER_I2C {
	pinctrl-0 = <&i2c00_scl_gpio004 &i2c00_sda_gpio003>;
	pinctrl-names = "default";
	port_sel = <0>;
};

/* typec-aic1-power */
&PD_POW_I2C {
	pinctrl-0 = <&i2c06_scl_gpio140 &i2c06_sda_gpio132>;
	pinctrl-names = "default";
	port_sel = <6>;
};
