/ {
	aliases {
		gpio-wp = &ec_spi_oe_mecc;
	};

	named-gpios {
		compatible = "named-gpios";

		/* Unimplemented GPIO */
		ec_spi_oe_mecc: ec-spi-wp {
			gpios = <MCHP_GPIO_DECODE_227 GPIO_INPUT>;
			enum-name = "GPIO_ENTERING_RW";
		};
	};
};
