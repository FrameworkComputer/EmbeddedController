/ {
	aliases {
		gpio-cbi-wp = &gpio_ec_cbi_wp;
		gpio-wp = &gpio_ec_flash_wp_odl;
	};

	named-i2c-ports {
		/delete-node/ ec_i2c5;
	};

	named-gpios {
		/* unimplemented GPIOs */
		entering_rw: entering_rw {
			enum-name = "GPIO_ENTERING_RW";
		};
	};
};

&pinctrl {
	i2c3_clk_gpf2_sleep: i2c3_clk_gpf2_sleep {
		pinmuxs = <&pinctrlf 2 IT8XXX2_ALT_DEFAULT>;
	};
	i2c3_data_gpf3_sleep: i2c3_data_gpf3_sleep {
		pinmuxs = <&pinctrlf 3 IT8XXX2_ALT_DEFAULT>;
	};
};

&i2c0 {
	label = "I2C_PWR_CBI";
	clock-frequency = <I2C_BITRATE_FAST>;
};

&i2c1 {
	label = "I2C_BATTERY";
	clock-frequency = <50000>;
};

&i2c2 {
	label = "I2C_USB_C0";
	clock-frequency = <I2C_BITRATE_FAST>;

};

&i2c3 {
	label = "I2C_SENSOR";
	clock-frequency = <I2C_BITRATE_FAST>;
	prescale-scl-low = <1>;
};

&i2c4 {
	label = "I2C_USB_C1";
	clock-frequency = <I2C_BITRATE_FAST>;
	prescale-scl-low = <1>;
};

&i2c5ALT {
	status = "disabled";
};

&pwm2 {
	prescaler-cx = <PWM_PRESCALER_C4>;
};

&pwm3 {
	prescaler-cx = <PWM_PRESCALER_C4>;
};
