		};
		ec_i2c3_usb_1_mix_sda {
			gpios = <&gpiod 0 GPIO_INPUT>;
			enum-name = "GPIO_EC_I2C3_USB_1_MIX_SDA";
		};
		ec_i2c5_power_scl {
			gpios = <&gpio3 3 GPIO_INPUT>;
			enum-name = "GPIO_EC_I2C5_BATTERY_SCL";
		};
		ec_i2c5_power_sda {
			gpios = <&gpio3 6 GPIO_INPUT>;
			enum-name = "GPIO_EC_I2C5_BATTERY_SDA";
