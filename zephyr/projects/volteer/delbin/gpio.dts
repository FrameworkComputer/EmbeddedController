			gpios = <&gpio3 4 (GPIO_INPUT | GPIO_PULL_UP)>;
		};
		unused_gpio41 {
			gpios = <&gpio4 1 (GPIO_INPUT | GPIO_PULL_UP)>;
		};
		unused_gpio72 {
			gpios = <&gpio7 2 (GPIO_INPUT | GPIO_PULL_UP)>;
		};
		unused_gpio96 {
