			enum-name = "GPIO_ENABLE_BACKLIGHT";
		};
		ec_i2c0_sensor_scl {
			gpios = <&gpiob 5 GPIO_INPUT>;
			enum-name = "GPIO_EC_I2C0_SENSOR_SCL";
		};
		ec_i2c0_sensor_sda {
			gpios = <&gpiob 4 GPIO_INPUT>;
			enum-name = "GPIO_EC_I2C0_SENSOR_SDA";
