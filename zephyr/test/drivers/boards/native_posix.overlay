};

&thermistor_3V0_22K6_47K_4050B  {
	status = "okay";
};

&thermistor_3V3_13K7_47K_4050B  {
	status = "okay";
};

&thermistor_3V3_51K1_47K_4050B  {
	status = "okay";
};

&flash0 {
        erase-block-size = <0x1000>;
        write-block-size = <1>;
        reg = <0x00000000 DT_SIZE_K(512)>;
};
