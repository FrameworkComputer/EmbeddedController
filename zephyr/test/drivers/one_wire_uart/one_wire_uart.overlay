#include <board-overlays/native_posix.dts>

/ {
	uart2 {
		compatible = "zephyr,uart-emul";
		status = "okay";

		one_wire_uart: one-wire-uart {
			compatible = "cros-ec,one-wire-uart";
			status = "okay";
			id = <0>;
		};
	};
};
