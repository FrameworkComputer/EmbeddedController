/ {
	/* Add a USB-A port to the board. Uses a GPIO pin defined in
	 * zephyr/test/drivers/boards/native_posix.overlay
	 */
	usba-port-enable-list {
		compatible = "cros-ec,usba-port-enable-pins";
		enable-pins = <&gpio_en_pp5000_usba_r>;
	};
};
