#include <board-overlays/native_sim.dts>

/ {
        chosen {
                cros-ec,keyboard = &fake_input_device;
        };

	fake_input_device: fake-device {
		compatible = "vnd,keyboard-input-device";
		col-size = <10>;
		row-size = <11>;

		kso_gpio: kso-gpio {
			compatible = "cros-ec,col-gpio";
			col-num = <2>;
			col-gpios = <&gpio0 0 GPIO_ACTIVE_HIGH>;
		};
	};
};
