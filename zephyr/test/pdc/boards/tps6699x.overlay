#include "native_sim.overlay"

/{
        /* Leverage 'zephyr,user' node for driver specific behavior */
        zephyr,user {
                test_timeout = <750>;
                /* TI PDC Info */
                pd_version = <0>;
                pd_revision = <0>;
                project_name = "GOOG";
        };
};

&i2c0 {
        pdc_emul1: tps6699-pdc@20 {
                compatible = "ti,tps6699-pdc";
                reg = <0x20>;
                irq-gpios = <&gpiof 5 GPIO_ACTIVE_LOW>;
        };

        pdc_emul2: tps6699-pdc@24 {
                compatible = "ti,tps6699-pdc";
                reg = <0x24>;
                irq-gpios = <&gpiof 5 GPIO_ACTIVE_LOW>;
        };
};
